<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Rasmed Publications Online Evaluation Administration Endpoint</title>

    <link href="~/css/admincss/bootstrap.css" rel="stylesheet" />
    <link href="~/css/admincss/Site.css" rel="stylesheet" />

    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div id="wrapper">

        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    @Html.ActionLink("Rasmed Publications Online Evaluation Administration", "Index", "WebAdmin", new { @class = "navbar-brand" })
                </div>
            </div>
        </div>


        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            @if (Request.IsAuthenticated)
            {

                <ul class="sidebar-nav">
                    <li class="sidebar-brand">
                        <a href="#">
                            Main Menu
                        </a>
                    </li>
                    <li>@Html.ActionLink("Home", "Index", "WebAdmin")</li>
                    <li>@Html.ActionLink("Examination", "Examination", "WebAdmin")</li>
                    <li>@Html.ActionLink("Result Manager", "ResultManagerStepOne", "WebAdmin")</li>
                    <li>@Html.ActionLink("Report", "Index", "WebAdmin")</li>
                  
                    <li>@Html.ActionLink("User Management", "Member", "WebAdmin")</li>


                </ul>
            }
        </div>
        <!-- /#sidebar-wrapper -->
        <!-- Page Content -->
        <div id="page-content-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        @if (TempData["notification"] != null)
                        {
                            <div class="Message">@TempData["notification"]</div>
                        } 
                        @RenderBody()
                        <hr/>
                    </div>
                </div>
            </div>
        </div>

    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")


    @RenderSection("scripts", required: false)


    <script src="http://code.jquery.com/ui/1.10.4/jquery-ui.min.js"></script>
    <script type='text/javascript'>
            $(function () {
                $('.date-picker').datepicker();
            })
    </script>
</body>
</html>
