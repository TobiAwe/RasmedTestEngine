@{
    ViewBag.Title = "Bulk Member Upload";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>@ViewBag.Title</h2>
<p></p>

@using (Html.BeginForm("MemberBulkUpload", "WebAdmin",
                FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    @Html.AntiForgeryToken()

    <div class="form-horizontal">


        @Html.ValidationSummary(true)

        <div class="form-group">


            <div><input type="file" name="MemberExelUpload" /></div>
            <p></p>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Upload Mwmber List" class="btn btn-success" />
                </div>
            </div>
        </div>
    </div>
}
